[core]
    editor = vim
[push]
    default = current
[alias]
    all = add -A
    p = pull
    s = status
    ss = status --short
    c = commit
    a = add
    d = diff --color-words='[^[:space:]]|([[:alnum:]]|UTF_8_GUARD)+'
    undo = checkout --
    some = add -u
    undocommit = reset "HEAD^"
[init]
	defaultBranch = master
